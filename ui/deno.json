{
  "name": "@betlang/ui",
  "version": "0.1.0",
  "exports": "./src/App.res.mjs",
  "tasks": {
    "dev": "deno run --allow-all npm:vite",
    "build": "deno run --allow-all npm:vite build",
    "preview": "deno run --allow-all npm:vite preview",
    "res:build": "deno run --allow-all npm:rescript build",
    "res:watch": "deno run --allow-all npm:rescript build -w",
    "res:clean": "deno run --allow-all npm:rescript clean"
  },
  "imports": {
    "rescript-tea": "npm:rescript-tea@^0.9.0",
    "rescript": "npm:rescript@^11.0.0",
    "vite": "npm:vite@^5.0.0"
  },
  "compilerOptions": {
    "lib": ["deno.window", "dom"],
    "jsx": "react-jsx",
    "jsxImportSource": "npm:react"
  }
}
